{
  "name": "@gigwork/ds",
  "version": "0.3.3",
  "description": "Basic Data Structures in TypeScript",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsup",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint src",
    "lint:fix": "tsc --noEmit && eslint src --fix",
    "lint:staged": "lint-staged",
    "test": "glob -c \"node --import tsx --test\" \"src/tests/*.test.ts\"",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "data-structures",
    "linked-list",
    "queue",
    "stack",
    "graph",
    "bloom-filter",
    "binary-tree",
    "b-tree",
    "trie",
    "red-black-tree",
    "disjoint-set",
    "heaps",
    "algorithms",
    "data-storage",
    "coding-tools"
  ],
  "author": "Igor Snitkin <sudo@snigo.dev>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/fnv-plus": "^1.3.2",
    "@types/imurmurhash": "^0.1.4",
    "@types/node": "^24.0.0",
    "eslint": "^10.0.0",
    "glob": "^13.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^16.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.0",
    "tslib": "^2.6.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@gigwork/eslint-config-service": "^1.0.3",
    "fnv-plus": "^1.3.1",
    "imurmurhash": "^0.1.4"
  }
}
